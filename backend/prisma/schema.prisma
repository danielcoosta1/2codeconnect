generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Publicacao {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  nome       String
  descricao  String
  imagem     String
  tags       String[]
  createdAt  DateTime @default(now())
}
